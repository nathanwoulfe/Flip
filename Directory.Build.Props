<Project>
	<PropertyGroup>
		<VersionPrefix>1.0.0</VersionPrefix>
		<VersionSuffix></VersionSuffix>
		<Authors>Nathan Woulfe</Authors>
		<Copyright>Nathan Woulfe 2021</Copyright>
		<PackageIconUrl></PackageIconUrl>

		<PackageProjectUrl>https://github.com/nathanwoulfe/blip</PackageProjectUrl>
		<PackageTags>Umbraco;UmbracoCms</PackageTags>

		<RepositoryUrl>https://github.com/nathanwoulfe/blip</RepositoryUrl>
		<RepositoryType>git</RepositoryType>

		<RestorePackagesWithLockFile>true</RestorePackagesWithLockFile>
		<RestoreLockedMode>false</RestoreLockedMode>
		<DisableImplicitNuGetFallbackFolder>true</DisableImplicitNuGetFallbackFolder>
		<NoWarn>$(NoWarn);NU5128;NU5125</NoWarn>
		<LangVersion>8.0</LangVersion>
	</PropertyGroup>
</Project>