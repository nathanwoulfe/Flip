<Project>
  <PropertyGroup>
    <VersionPrefix>10.0.0</VersionPrefix>
    <VersionSuffix></VersionSuffix>
    <Authors>Nathan Woulfe</Authors>
    <Copyright>Copyright Â© Nathan Woulfe $([System.DateTime]::Today.ToString('yyyy'))</Copyright>
    <PackageIcon>icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>

    <PackageProjectUrl>https://github.com/nathanwoulfe/clip</PackageProjectUrl>
    <RepositoryUrl>https://github.com/nathanwoulfe/clip</RepositoryUrl>
    <RepositoryType>git</RepositoryType>

    <NoWarn>$(NoWarn);NU5128;NU5125</NoWarn>
    <LangVersion>10.0</LangVersion>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="$(MSBuildThisFileDirectory)icon.png" Pack="true" PackagePath="" />
    <None Include="$(MSBuildThisFileDirectory)README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>
</Project>